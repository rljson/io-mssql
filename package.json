{
  "name": "@rljson/io-mssql",
  "version": "0.0.21",
  "description": "Rljson io-mssql description",
  "homepage": "https://github.com/rljson/io-mssql",
  "bugs": "https://github.com/rljson/io-mssql/issues",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rljson/io-mssql.git"
  },
  "main": "dist/io-mssql.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "pnpm vite build && tsc && node scripts/copy-readme-to-dist.js",
